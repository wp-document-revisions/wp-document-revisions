{
	"name": "wp-document-revisions",
	"version": "3.6.0",
	"description": "A document management and version control plugin for WordPress",
	"main": "dist/index.js",
	"scripts": {
		"build": "webpack --mode=production",
		"dev": "webpack --mode=development",
		"watch": "webpack --mode=development --watch",
		"lint": "eslint src --ext .ts,.tsx,.js,.jsx",
		"lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"type-check": "tsc --noEmit",
		"test": "jest",
		"test:all": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"all": "npm run lint && npm run format && npm run type-check && npm run test && npm run build",
		"all:ci": "npm run lint && npm run format:check && npm run type-check && npm run test && npm run build",
		"precommit": "npm run lint && npm run format:check && npm run type-check && npm test -- --bail",
		"qa": "npm run lint && npm run format:check && npm run type-check && npm run test:coverage && npm run build",
		"prepare": "husky install",
		"release:dry": "node script/release --dry-run || echo 'Add script/release implementation'",
		"e2e:bootstrap": "node script/wp-bootstrap.js",
		"e2e:test": "playwright test",
		"e2e:test:headed": "playwright test --headed",
		"e2e:report": "playwright show-report",
		"e2e:update": "playwright test --update-snapshots"
	},
	"keywords": [
		"wordpress",
		"plugin",
		"document",
		"revision",
		"management"
	],
	"author": "Ben Balter",
	"license": "GPL-3.0",
	"devDependencies": {
		"@testing-library/jest-dom": "^6.4.8",
		"@testing-library/react": "^16.0.0",
		"@testing-library/user-event": "^14.5.2",
		"@types/jest": "^29.5.12",
		"@types/jquery": "^3.5.32",
		"@types/node": "^20.14.11",
		"@typescript-eslint/eslint-plugin": "^7.16.1",
		"@typescript-eslint/parser": "^7.16.1",
		"@wordpress/eslint-plugin": "^19.0.0",
		"@wordpress/e2e-test-utils-playwright": "^1.28.0",
		"css-loader": "^7.1.2",
		"eslint": "^8.57.0",
		"eslint-config-wordpress": "^2.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"husky": "^9.0.0",
		"lint-staged": "^15.2.9",
		"prettier": "^3.3.3",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"mini-css-extract-plugin": "^2.9.0",
		"playwright": "^1.47.0",
		"@playwright/test": "^1.47.0",
		"dotenv": "^16.4.5",
		"sass": "^1.77.8",
		"sass-loader": "^16.0.0",
		"ts-jest": "^29.4.1",
		"ts-loader": "^9.5.1",
		"typescript": "5.5.4",
		"webpack": "^5.93.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"@wordpress/blocks": "^13.6.0",
		"@wordpress/components": "^28.6.0",
		"@wordpress/element": "^6.6.0",
		"@wordpress/i18n": "^5.6.0"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx,js,jsx}": [
			"eslint --ext .ts,.tsx,.js,.jsx",
			"prettier --check"
		],
		"*.{json,md,yml,yaml}": [
			"prettier --check"
		]
	}
}
